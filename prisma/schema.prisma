generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                      String   @id
  email                   String   @unique
  password                String
  name                    String
  description             String
  birthDate               DateTime
  coffeeCounter           Int?
  coffeeExtraction        String?
  coffeeGrinderMachine    String?
  coffeeExtractionMachine String?
}
